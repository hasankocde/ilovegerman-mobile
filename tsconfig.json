{
  "compilerOptions": {
    "incremental": true,
    "target": "es2022",
    "module": "esnext",
    "lib": [
      "dom",
      "es2022"
    ],
    "jsx": "react-jsx",
    "strict": true,
    "sourceMap": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/renderer/FastTranslate/*"
      ],
      "@renderer/*": [
        "src/renderer/*"
      ]
    }
  },
  "exclude": [
    "test",
    "release/build",
    "release/app/dist",
    "dist",
    "dist-electron"
  ]
}